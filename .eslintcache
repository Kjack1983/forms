teReducer = (state, action) => {\n    switch(action?.type) {\n        case actionTypes.INPUT:\n            return { \n                value: action.value,\n                isTouched: state.isTouched\n            }\n        case actionTypes.BLUR:\n            return {\n                value: state.value,\n                isTouched: true\n            }\n        case actionTypes.RESET:\n            return {\n                value: '',\n                isTouched: false\n            }\n        default:\n            console.log(`NO TYPE PROVIDED`.red);\n            break\n    }\n}\n\nexport default function useInput(validateValue) {\n\n    if(typeof validateValue !== 'function') {\n        throw new Error('Argument is not a function');\n    }\n\n    const [inputState, dispatch] = useReducer(inputStateReducer, initialInputState);\n\n    const valueIsValid = validateValue(inputState.value);\n    const hasError = !valueIsValid && inputState.isTouched;\n\n    const valueChangeHandler = (event) => {\n        dispatch({type: 'INPUT', value: event.target.value});\n    }\n\n    const inputBlurHandler = () => {\n        dispatch({type: 'BLUR'});\n    }\n\n    const reset = () => {\n        dispatch({type: 'RESET'})\n    }\n\n    return { \n        value: inputState.value, \n        hasError,\n        isValid: valueIsValid, \n        valueChangeHandler, \n        inputBlurHandler,\n        reset\n    };\n}\n","/home/kjack/development/udemyprojects/forms/src/components/BasicForm.js",[],{"ruleId":"29","replacedBy":"30"},{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","severity":1,"message":"34","line":6,"column":12,"nodeType":"35","messageId":"36","endLine":6,"endColumn":23},{"ruleId":"33","severity":1,"message":"37","line":16,"column":12,"nodeType":"35","messageId":"36","endLine":16,"endColumn":24},{"ruleId":"33","severity":1,"message":"38","line":1,"column":8,"nodeType":"35","messageId":"36","endLine":1,"endColumn":13},"no-native-reassign",["39"],"no-negated-in-lhs",["40"],"no-unu